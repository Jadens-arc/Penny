      "oneClick": false
    }
  },
  "keywords": [],
  "author": "Jaden arceneaux",
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto": "^1.0.1"
  }
}
